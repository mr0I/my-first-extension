{
    "name": "__MSG_appName__",
    "version": "1.0.0",
    "description": "__MSG_appDesc__",
    "manifest_version": 2,
    "default_locale": "en",
    "background": {
        "scripts": [
            "/src/background.js",
            "/src/backgroundMessaging.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "js": [
                "/src/content.js",
                "/src/contentMessaging.js",
                "/src/contentRequests.js"
            ],
            "matches": [
                "https://*.sisoog.com/*"
            ],
            "exclude_matches": [
                "https://*.sisoog.com/category/*"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "src/assets/images/logo.png",
        "default_title": "Opens MyExtension",
        "default_popup": "/src/app.html"
    },
    "icons": {
        "16": "src/assets/images/logo.png",
        "48": "src/assets/images/logo.png",
        "128": "src/assets/images/logo.png"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "storage",
        "https://api.github.com/"
    ]
}